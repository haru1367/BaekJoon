import java.util.Scanner;
public class Main {
    static long[] dp = new long[46];
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        Main.dp[0] = 0;
        Main.dp[1] = 1;
        for (int i = 2; i<46; i++) {
            Main.dp[i] = Main.dp[i-2] + Main.dp[i-1];
        }
        System.out.println(Main.dp[n]);
        sc.close();
    }
}
