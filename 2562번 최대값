#include <iostream>
#include <vector>
using namespace std;
int main() {
	int maxn = 0;
	vector<int>num;
	int result = 0;
	while (true) {
		int N;
		cin >> N;
		if (cin.eof() == true) {
			break;
		}
		else {
			num.push_back(N);
			if (N > maxn) {
				maxn = N;
				result = num.size();
			}
		}
	}
	cout << maxn << endl;
	cout << result << endl;
}
