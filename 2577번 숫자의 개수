#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main() {
	int a, b, c;
	cin >> a >> b >> c;
	int num = a * b * c;
	string str = to_string(num);
	vector<int>m(10, 0);
	for (int i = 0; i < str.size(); i++) {
		m[str[i] - 48]++;
	}
	for (int i = 0; i < m.size(); i++) {
		cout << m[i] << endl;
	}
}
