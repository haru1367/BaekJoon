#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main() {
	string A, B;
	cin >> A >> B;
	if (A.size() < B.size()) {
		string s = B;
		B = A;
		A = s;
	}
	vector<int>num1;
	vector<int>num2;
	for (int i = 0; i < A.size() - B.size(); i++) {
		num2.push_back(0);
	}
	for (int i = 0; i < A.size(); i++) {
		num1.push_back(A[i] - 48);
	}
	for (int i = 0; i < B.size(); i++) {
		num2.push_back(B[i]-48);
	}
	
	for (int i = num1.size() - 1; i >= 1; i--) {
		if (num1[i] + num2[i] >= 10) {
			num1[i] = num1[i] + num2[i] - 10;
			num1[i - 1]++;
		}
		else {
			num1[i] = num1[i] + num2[i];
		}
	}
	string str = "";
	str += to_string(num1[0] + num2[0]);
	for (int i = 1; i <= num1.size() - 1; i++) {
		str += to_string(num1[i]);
	}
	cout << str << endl;
}
