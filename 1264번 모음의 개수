#include <iostream>
#include <string>
using namespace std;
int main() {
	while (true) {
		string str;
		getline(cin, str, '\n');
		if (str == "#") {
			break;
		}
		int num = 0;
		for (int i = 0; i < str.size(); i++) {
			if (str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u'
				|| str[i] =='A' || str[i] =='E' || str[i] == 'I' || str[i] == 'O' || str[i] == 'U') {
				num++;
			}
		}
		cout << num << '\n';
	}
}
