#include <iostream>
#include <string>
#include <cmath>
using namespace std;
int main() {
	int N;
	cin >> N;
	string str;
	cin >> str;
	long long temp=1;
	long long sum = 0;
	for (int i = 0; i < str.size(); i++) {
		sum += (str[i]-'a'+1) * temp;
		temp = (temp * 31) % 1234567891;
	}
	cout << sum%1234567891 << '\n';
}
