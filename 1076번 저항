#include <iostream>
#include <algorithm>
#include <string>
#include <map>
using namespace std;
int main() {
	map<string, pair<int,long long>>m;
	string A, B, C;
	cin >> A >> B >> C;
	m["black"] = {0,1};
	m["brown"] = { 1,10 };
	m["red"] = { 2,100 };
	m["orange"] = { 3,1000 };
	m["yellow"] = { 4,10000 };
	m["green"] = { 5,100000 };
	m["blue"] = { 6,1000000 };
	m["violet"] = { 7,10000000 };
	m["grey"] = { 8,100000000 };
	m["white"] = { 9,1000000000 };
	long long a = m[A].first * 10 + m[B].first;
	cout << a * m[C].second << '\n';
}
