#include <iostream>
#include <string>
using namespace std;
int main(){
    int N;
    cin>>N;
    for (int i = 1; i<=N; i++) {
        string str;
        cin>>str;
        if (str.size() >= 6 && str.size() <= 9) {
            cout << "yes" <<'\n';
        } else {
            cout << "no" <<'\n';
        }
    }
}
