#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
using namespace std;
int main() {
	while (true) {
		int a, b, c;
		cin >> a >> b >> c;
		if (a == b && b == c && a == 0) break;
		vector<int>num;
		num.push_back(a);
		num.push_back(b);
		num.push_back(c);
		sort(num.begin(), num.end());
		if (pow(num[0], 2) + pow(num[1], 2) == pow(num[2], 2)) {
			cout << "right" << '\n';
		}
		else {
			cout << "wrong" << '\n';
		}
	}
}
