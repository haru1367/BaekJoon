#include <iostream>
#include <string>
using namespace std;
int main() {
	int T;
	cin >> T;
	for (int i = 1; i <= T; i++) {
		int H, W, N;
		cin >> H >> W >> N;
		int alpha;
		string str = "";
		if (N % H == 0) {
			str += to_string(H);
			alpha = N / H;
		}
		else {
			str += to_string(N % H);
			alpha = N / H + 1;			
		}
		if (alpha < 10) {
			str += '0';
			str += to_string(alpha);
		}
		else {
			str += to_string(alpha);
		}
		cout << stoi(str) << endl;
	}
}
