#include <iostream>
#include <string>
using namespace std;
int main(){
    string str;
    cin>> str;
    string sstr = "";
    int count = 0;
    for (int i = 0; i<str.size(); i++) {
        sstr += str[i];
        count++;
        if (count % 10 == 0) {
            cout << sstr << '\n';
            sstr = "";
        }
    }
    if (sstr.size()!= 0) {
        cout << sstr << '\n';
    }
}
