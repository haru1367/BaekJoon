#include <iostream>
#include <string>
using namespace std;
int main(){
    string a;
    cin>>a;
    for (char s = 'a'; s<='z'; s++) {
        int num = 0;
        for (int i = 0; i<a.size(); i++) {
            if (a[i] == s) {
                num++;
            }
        }
        cout << num << " ";
    }
}
