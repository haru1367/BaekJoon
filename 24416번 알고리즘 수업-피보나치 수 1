import java.util.Scanner;


public class Main {
    static long[] dp = new long[41];

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       dp[1] = 1;
       dp[2] = 1;
       for (int i = 3; i<=40; i++) {
           dp[i] = dp[i-2]+dp[i-1];
       }
       System.out.print(dp[n]);
       System.out.print(" ");
       System.out.print(n-2);

    }
}
