#include <iostream>
#include <cmath>
using namespace std;
int arr[3][4];
int main() {
	for (int i = 1; i <= 2; i++) {
		for (int j = 1; j <= 3; j++) {
			int k;
			cin >> k;
			arr[i][j] = k;
		}
	}
	cout << abs(arr[2][1] - arr[1][3]) << " " << arr[2][2] / arr[1][2] << " " << abs(arr[2][3] - arr[1][1]);
}
